<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
  </PropertyGroup>

  <PropertyGroup>
    <PackageId>Rstolsmark.EncryptionLib</PackageId>
    <Version>1.0.1</Version>
    <Authors>ragnarstolsmark</Authors>
    <Company>Rstolsmark</Company>
    <Description>A library for encrypting and decrypting strings using AES.</Description>
    <PackageTags>encryption;AES;decryption</PackageTags>
    <RepositoryUrl>https://github.com/ragnarstolsmark/Rstolsmark.EncryptionLib</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Cryptography.KeyDerivation" Version="2.2.0" />
  </ItemGroup>

</Project>
